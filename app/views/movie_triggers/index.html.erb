<p id="notice"><%= notice %></p>

<h1>Movie Triggers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movie_triggers.each do |movie_trigger| %>
      <tr>
        <td><%= link_to 'Show', movie_trigger %></td>
        <td><%= link_to 'Edit', edit_movie_trigger_path(movie_trigger) %></td>
        <td><%= link_to 'Destroy', movie_trigger, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movie Trigger', new_movie_trigger_path %>
